<!DOCTYPE html>
<html lang="pt-BR">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>CRUD Node.js</title>
  <link rel="shortcut icon" href="img/icon.png" type="image/x-icon">
  <link rel="stylesheet" href="css/style.css">
  <link rel="stylesheet" href="css/nav.css">
  <script src="https://kit.fontawesome.com/20f8330ac2.js" crossorigin="anonymous"></script>

</head>
<body id="body" onload="open_close_menu()">
  <header>
      <div class="icon__menu">
          <i class="fas fa-bars" id="btn_open"></i>
      </div>
  </header>
  <div class="menu__side" id="menu_side"> 
      
      <div class="name__page"> 
          <i class="fa-solid fa-shield-dog"></i>
          <h4>MyVet</h4>
      </div>
      <div class="options__menu">
              <a href="/index" >
                  <div class="option">
                      <i class="fas fa-home" title="Inicio"></i>
                      <h4>Inicio</h4> 
                  </div>
              </a>
              <a href="./addPaciente" >
                  <div class="option">
                      <i class="fa-solid fa-paw" title="Paciente"></i>
                      <h4>Pacientes</h4>
                  </div>
              </a>
              <a href="./Client" class="selected">
                  <div class="option">
                      <i class="far fa-id-badge" title="Cliente"></i>
                      <h4>Cliente</h4>
                  </div>
              </a>
              <a href="./query" >
                  <div class="option">
                      <i class="fa-solid fa-file-lines" title="Consulta"></i>
                      <h4>Consulta</h4>
                  </div>
              </a>
              <a href="./ListLogin">
                  <div class="option">
                      <i class="fa-solid fa-stethoscope" title="Usuarios"></i>
                      <h4>Usuarios</h4> 
                  </div>
              </a>
              <a href="./product">
                <div class="option">
                    <i class="fa-brands fa-product-hunt" title="Producto"></i>
                    <h4>Productos</h4> 
                </div>
            </a>
            <a href="./Category">
              <div class="option">
                  <i class="fa-solid fa-c" title="Producto"></i>
                  <h4>Categorias</h4> 
              </div>
            </a>
                <a href="./" class="logout">
                    <div class="option">
                        <i class="fa-solid fa-right-from-bracket" title="Salir"></i>
                        <h4>Cerrar Sesión</h4> 
                    </div>
                </a>
      </div>
  </div>
  <div class="form-register">
      <h2>Ingresar Cliente</h2>
      <hr>
      <br>
    <br>
    <form action="./addClient" method="POST" class="container-form">
      <div class="container-input">
        <div class="input">
          <label for="clientname" class="form-label">Nombre del cliente</label>
          <input class="controls" type="text" name="clientname" id="clientname" class="form-control">
        </div>
        <div class="input">
          <label for="dni" class="form-label">DNI</label>
          <input class="controls" type="text" name="dni" id="dni" class="form-control">
        </div>        
      </div>
      <div class="container-input">
        <div class="input">
          <label for="email" class="form-label">Email</label>
          <input class="controls" type="text" name="email" id="email" class="form-control">
        </div>
        <div class="input">
          <label for="phone" class="form-label">Telefono</label>
          <input class="controls" type="text" name="phone" id="phone" class="form-control">
        </div>
      </div>
      <div class="container-input">
        <div class="input">
          <label for="city" class="form-label">Departamento</label>
          <select class="controls" name="city" id="city" class="form-select">
            <option value="" selected disabled>Seleccione su departamento</option>
            <option value="Capital">Capital</option>
            <option value="Gral Alvear">General Alvear</option>
            <option value="Godoy Cruz">Godoy Cruz</option>
            <option value="Guaymallén">Guaymallén</option>
            <option value="Junín">Junín</option>
            <option value="Lavalle">Lavalle</option>
            <option value="Las Heras">Las Heras</option>
            <option value="La Paz">La Paz</option>
            <option value="Luján de cuyo">Luján de cuyo</option>
            <option value="Maipú">Maipú</option>
            <option value="Malargüe">Malargüe</option>
            <option value="Rivadavia">Rivadavia</option>
            <option value="San Carlos">San Carlos</option>
            <option value="Santa Rosa">Santa Rosa</option>
            <option value="Tunuyán">Tunuyán</option>
            <option value="Tupungato">Tupungato</option>
          </select>
        </div> 
      </div>
      <div class="container-btn">
          <div>
            <button type="submit" class="boton">Agregar Cliente</button>
          </div>
          <div>
            <a href="./Client" class="boton">Volver</a>
          </div>
        </div>
      </div>
    </form>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-gtEjrD/SeCtmISkJkNUaaKMoLD0//ElJ19smozuHV6z3Iehds+3Ulb9Bn9Plx0x4"
    crossorigin="anonymous"></script>
  <script src="js/script.js"></script>
  <script src="js/menu.js"></script>

</body>

</html>